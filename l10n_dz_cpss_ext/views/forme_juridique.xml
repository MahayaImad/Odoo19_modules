<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="forme_juridique_view_form" model="ir.ui.view">
        <field name="name">forme.juridique.view.form</field>
        <field name="model">forme.juridique</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="forme_juridique_view_tree" model="ir.ui.view">
        <field name="name">forme.juridique.view.tree</field>
        <field name="model">forme.juridique</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>

